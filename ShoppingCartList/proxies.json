{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHomePage": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://storageaccountfunct991b.z20.web.core.windows.net/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/css/{*restOfPath}"
      },
      "backendUri": "https://storageaccountfunct991b.z20.web.core.windows.net/css/{restOfPath}"
    }
  }
} 